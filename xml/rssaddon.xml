<?xml version="1.0" encoding="UTF-8"?>
<mgrdata>
   <library name="rssaddon" />

    <metadata name="rss.list" type="list" key="name" mgr="billmgr">
      <toolbar view="buttontext">
        <toolgrp name="new">
          <toolbtn name="fetch" func="rss.fetch" type="refresh" img="t-new" sprite="yes"/>
        </toolgrp>
      </toolbar>
      <coldata>
        <col name="title" type="data" sort="alpha"/>
        <col name="pubDate" type="data" sort="alpha"/>
        <col name="link" type="data" sort="alpha"/>
      </coldata>
    </metadata>

    <tparams>
      <out>devel</out>
      <func>rss.list</func>
    </tparams>

    <tparams>
      <out>devel</out>
      <func>rss.fetch</func>
    </tparams>
</mgrdata>
